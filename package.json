{
  "name": "ixo-oracles-cli",
  "version": "1.0.1",
  "description": "CLI tool for creating and managing IXO Oracle projects",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "oracles-cli": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "cli",
    "ixo",
    "blockchain",
    "oracles",
    "ai",
    "matrix",
    "signx"
  ],
  "author": "IXO Foundation",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ixoworld/ixo-oracles-cli.git"
  },
  "bugs": {
    "url": "https://github.com/ixoworld/ixo-oracles-cli/issues"
  },
  "homepage": "https://github.com/ixoworld/ixo-oracles-cli#readme",
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@cosmjs/crypto": "^0.34.0",
    "@cosmjs/encoding": "^0.34.0",
    "@cosmjs/proto-signing": "^0.34.0",
    "@ixo/impactxclient-sdk": "^2.4.1",
    "@ixo/matrixclient-sdk": "^0.3.4",
    "@ixo/signx-sdk": "^1.2.0",
    "@matrix-org/matrix-sdk-crypto-wasm": "^15.1.0",
    "eciesjs": "^0.4.15",
    "loglevel": "^1.9.2",
    "matrix-js-sdk": "^37.12.0",
    "md5": "^2.3.0",
    "multiformats": "^13.3.7",
    "node-fetch": "^3.3.2",
    "qrcode-terminal": "^0.12.0",
    "simple-git": "^3.28.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.0.0",
    "@types/qrcode-terminal": "^0.12.2",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.0"
  },
  "pnpm": {
    "overrides": {
      "node-fetch": "^3.3.2"
    }
  }
